<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.mapper.TBookMapper">
    <resultMap id="TBookMap" type="com.example.demo.pojo.TBookPO">
        <id column="UserId" property="userId" jdbcType="INTEGER"></id>
        <result column="userName" property="userName" jdbcType="VARCHAR" />
        <result column="password" property="password" jdbcType="VARCHAR" />
        <result column="phone" property="phone" jdbcType="VARCHAR" />
    </resultMap>
    <select id="queryUserList" parameterType="com.example.demo.pojo.TBookPO" resultType="com.example.demo.pojo.TBookPO">
        select * from t_user
        where 1=1
        <if test="userId != null">
            and  UserId = #{userId, jdbcType=INTEGER}
        </if>
        <if test="userName!=null">
            and userName = #{userName, jdbcType=VARCHAR}
        </if>
        <if test="password!=null">
            and password = #{passeord, jdbcType=VARCHAR}
        </if>
        <if test="phone!=null">
            and phone = #{phone, jdbcTyoe=VARCHAR}
        </if>
        ;
    </select>
</mapper>